<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp">


    <com.icollection.fonts.MerriweatherTextView
        android:id="@+id/merriweatherTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:gravity="center"
        android:text="MENU UTAMA"
        android:textColor="@color/colorTextSecondary"
        android:textSize="22sp"
        app:typeface="merriWeatherRegular" />

    <Button
        android:id="@+id/tblLihat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Lihat Detail" />

    <Button
        android:id="@+id/tblTagih"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Kunjungan" />

    <Button
        android:id="@+id/tblTidakKunjungan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Tidak Kunjungan" />
    <Button
        android:id="@+id/tblBHC"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="BHC" />

</LinearLayout>
