<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp">


    <com.icollection.fonts.MerriweatherTextView
        android:id="@+id/merriweatherTextView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:gravity="center"
        android:text="TIDAK KUNJUNGAN"
        android:textColor="@color/colorTextSecondary"
        android:textSize="22sp"
        app:typeface="merriWeatherRegular" />

    <Button
        android:id="@+id/tblSudahBayar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SUDAH BAYAR" />

    <Button
        android:id="@+id/tblJanji"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Janji Bayar" />

    <Button
        android:id="@+id/tblTidakSempat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TIDAK SEMPAT" />


</LinearLayout>
